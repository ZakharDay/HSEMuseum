<div class="galleryArtwork">
  <h2><%= link_to artwork.title, artwork %></h2>

  <p>
    <strong>Year:</strong>
    <%= artwork.year %>
  </p>

  <%= image_tag artwork.image %>

  <%= link_to 'Edit', edit_artwork_path(artwork) %>
  <%= link_to 'Destroy', artwork, method: :delete, data: { confirm: 'Are you sure?' } %>
</div>
